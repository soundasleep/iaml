«IMPORT iaml»

«EXTENSION template::GeneratorExtensions»

«DEFINE setSessionPhp FOR model::InternetApplication-»
<?php
/**
 * For setting a session variable.
 */
 
require("header.php");

log_message("set_session.php? " . print_r($_GET, true));
$id = $_GET["id"] or throw_new_IamlRuntimeException("no id");
$arg0 = $_GET["arg0"] or throw_new_IamlRuntimeException("no arg0");

// set
$_SESSION[$id] = $arg0;

echo "ok";
log_message("set_session.php completed");

«ENDDEFINE»
