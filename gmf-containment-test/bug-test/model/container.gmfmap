<?xml version="1.0" encoding="UTF-8"?>
<gmfmap:Mapping xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="test.ecore#//Container/elements"/>
    <ownedChild>
      <domainMetaElement
          href="test.ecore#//Element"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="test.gmfgraph#ElementName"/>
        <features
            href="test.ecore#//Element/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="container.gmftool#//@palette/@tools.0/@tools.1"/>
      <diagramNode
          href="test.gmfgraph#Element"/>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="test.ecore#//Edge"/>
    <containmentFeature
        href="test.ecore#//ContainsEdges/edges"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="container.gmftool#//@palette/@tools.0/@tools.0"/>
    <diagramLink
        href="test.gmfgraph#Edge"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="test.ecore#//Edge/from"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="test.ecore#//Edge/to"/>
  </links>
  <diagram>
    <diagramCanvas
        href="test.gmfgraph#test"/>
    <domainModel
        href="test.ecore#/"/>
    <domainMetaElement
        href="test.ecore#//Container"/>
    <palette
        href="container.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
