«IMPORT iaml»

«EXTENSION template::GeneratorExtensions»

«DEFINE clearSessionPhp FOR model::InternetApplication-»
<?php
/**
 * Clear the existing session.
 */
 
require("config.php");		// runtime library configuration
require(IAML_RUNTIME_ROOT."global.php");	// runtime library import

log_message("clear_session.php? " . print_r($_GET, true));

foreach ($_SESSION as $k => $v) {
	unset($_SESSION[$k]);
}

echo "ok";
log_message("clear_session.php completed");

«ENDDEFINE»
