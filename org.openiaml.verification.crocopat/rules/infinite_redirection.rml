
RedirectsTo(a, b) :=
  Frame(a) & a != "null" &
  EX(e, EventTrigger(e) &
  onAccess(a, e) &
  Frame(b) & b != "null" &
  EX(w, ActionEdge(w) & from(w, e) & to(w, b)));

RedirectsTo(a, b) :=
  Frame(a) & Frame(b) & TC(RedirectsTo(a, b));

InfiniteRedirect(p) := 
  Frame(p) & p != "null" &
  RedirectsTo(p, p);
  
PRINT ["Infinite redirection"] InfiniteRedirect(p);
