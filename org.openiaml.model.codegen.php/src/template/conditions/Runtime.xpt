«IMPORT iaml»

/**
 * Runtime checks of conditions.
 */
«EXTENSION template::GeneratorExtensions»

«DEFINE callCondition(Boolean php, Boolean inStoreDb) FOR model::SimpleCondition»
	/* '«simpleName(function)»' */
	condition_«conditionName(function)»(
		«EXPAND template::operations::Parameters::callParameter(php, inStoreDb, false) FOREACH inParameterEdges SEPARATOR ", "»
	)
«ENDDEFINE»

«DEFINE exception FOR Object»
«ENDDEFINE»
